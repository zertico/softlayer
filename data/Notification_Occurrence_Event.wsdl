      <output message="tns:getStatusCodeResponse"/>
    </operation>
    <operation name="getUpdates">
      <documentation>Retrieve all updates for this event.</documentation>
      <input message="tns:getUpdates"/>
      <output message="tns:getUpdatesResponse"/>
    </operation>
  </portType>
